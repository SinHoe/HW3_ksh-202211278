    "def factorial(a):\n",
    "\n",
    "    result = 1\n",
    "    if (a !=0 ):\n",
    "        for i in range(a,1,-1):\n",
    "              result = result * i\n",
    "    return result\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "\n",
    "    for i in range(0,15,2):\n",
    "        print(i,\"! = \",factorial(i))\n",
    "\n"
   ]
